{
    "name": "@jahia/app-shell",
    "version": "1.4.0",
    "scripts": {
        "build": "yarn webpack:library --mode=development && yarn webpack:appshell --mode=development",
        "prebuild:production": "yarn lint && yarn test",
        "build:production": " yarn webpack:library --mode=production && yarn webpack:appshell --mode=production",
        "webpack:appshell": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js",
        "webpack:library": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js --config webpack.libraries.config.js",
        "watch": "yarn webpack --watch",
        "clean": "rimraf *.log src/main/resources/javascript/apps",
        "clean:all": "yarn clean && rimraf node_modules node target",
        "lint": "eslint --ext js,jsx .",
        "lint:fix": "eslint --ext js,jsx --fix .",
        "test": "jest --ci --run-in-band --coverage",
        "tdd": "jest --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "description": "Jahia app-shell - the heart of modularization and micro-front-end",
    "main": "index.js",
    "repository": "git@github.com:Jahia/app-shell.git",
    "author": "Jahia Core Team <core@jahia.com>",
    "license": "SEE LICENSE IN LICENSE.txt FILE",
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@jahia/data-helper": "^0.0.24",
        "@jahia/design-system-kit": "^1.1.7",
        "@jahia/icons": "^1.1.1",
        "@jahia/moonstone": "^0.16.0",
        "@jahia/react-material": "^2.1.2",
        "@jahia/ui-extender": "^0.4.18",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link": "^1.2.13",
        "apollo-link-batch-http": "^1.2.13",
        "apollo-link-http": "^1.5.16",
        "apollo-utilities": "^1.3.3",
        "graphql": "^14.6.0",
        "graphql-tag": "^2.10.3",
        "i18next": "^19.0.3",
        "i18next-chained-backend": "^2.0.1",
        "i18next-xhr-backend": "^3.2.2",
        "mdi-material-ui": "^5.10.0",
        "react": "^16.12.0",
        "react-apollo": "^3.1.3",
        "react-dom": "^16.12.0",
        "react-file-viewer": "^1.1.0",
        "react-i18next": "^11.2.7",
        "react-iframe": "^1.8.0",
        "react-pdf-js": "^4.0.2",
        "react-redux": "^7.1.3",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.5",
        "redux-batched-actions": "^0.4.1",
        "redux-thunk": "^2.3.0",
        "rxjs": "^6.3.3",
        "whatwg-fetch": "^3.0.0",
        "zen-observable": "^0.8.15"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/plugin-external-helpers": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.6",
        "@babel/preset-react": "^7.8.3",
        "@jahia/eslint-config": "^1.1.0",
        "@jahia/test-framework": "^1.1.5",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-lodash": "^3.3.4",
