{
    "name": "@jahia/dx-commons-webpack",
    "version": "1.4.0",
    "scripts": {
        "build": "yarn lint && yarn webpack && yarn webpack:manifest",
        "webpack": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js",
        "webpack:manifest": "yarn webpack --manifest",
        "build:analyze": "yarn webpack --analyze",
        "build:production": "yarn lint && yarn webpack -p && yarn webpack:manifest -p",
        "build:production-analyze": "yarn webpack -p --analyze",
        "watch": "yarn webpack --watch",
        "clean": "rimraf *.log src/main/resources/javascript/apps",
        "clean:all": "yarn clean && rimraf node_modules node target",
        "lint": "eslint --ext js,jsx .",
        "lint:fix": "eslint --ext js,jsx --fix ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "description": "DX Commons Javascript loader",
    "main": "index.js",
    "repository": "git@github.com:Jahia/dx-commons-webpack.git",
    "author": "Jahia Core Team <core@jahia.com>",
    "license": "SEE LICENSE IN LICENSE.txt FILE",
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@jahia/apollo-dx": "^1.1.0",
        "@jahia/design-system-kit": "^1.1.2",
        "@jahia/icons": "^1.1.0",
        "@jahia/react-apollo": "^1.1.0",
        "@jahia/react-material": "^1.1.2",
        "@jahia/moonstone": "^0.3.0",
        "@jahia/registry": "^1.0.0-beta.20190724063719",
        "i18next": "^19.0.3",
        "i18next-chained-backend": "^2.0.1",
        "i18next-xhr-backend": "^3.2.2",
        "mdi-material-ui": "^5.10.0",
        "react": "^16.12.0",
        "react-apollo": "^2.1.3",
        "react-dom": "^16.12.0",
        "react-file-viewer": "^1.1.0",
        "react-i18next": "^11.2.7",
        "react-pdf-js": "^4.0.2",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.0",
        "rxjs": "^6.3.3",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
