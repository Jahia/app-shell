    "@jahia/layouts": "^1.0.0-beta.20190307142048",
    "@jahia/react-apollo": "^1.0.0-beta.20181113171845",
    "@jahia/react-material": "^1.0.0-beta.20190308153704",
    "@jahia/registry": "^1.0.0-beta.20190130110253",
    "babel-plugin-transform-imports": "^1.5.1",
    "react-apollo": "^2.1.3",
    "redux": "^4.0.0",
    "rxjs": "^6.3.3",
    "transform-imports": "^2.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-external-helpers": "^7.0.0",
