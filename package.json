{
    "name": "@jahia/dx-commons-webpack",
    "version": "1.2.0",
    "scripts": {
        "build": "yarn test && yarn webpack && yarn webpack:manifest",
        "webpack": "node --max_old_space_size=2048 ./node_modules/.bin/webpack",
        "webpack:manifest": "node --max_old_space_size=2048 ./node_modules/.bin/webpack --manifest",
        "build:analyze": "yarn build --analyze",
        "build:production": "yarn test && yarn webpack -p && yarn webpack:manifest -p",
        "build:production-analyze": "yarn build -p --analyze",
        "watch": "yarn webpack --watch",
        "clean": "rimraf *.log src/main/resources/javascript/apps",
        "clean:all": "yarn clean && rimraf node_modules node target",
        "test": "yarn lint",
        "lint": "eslint --ext js,jsx .",
        "lint:fix": "eslint --ext js,jsx --fix ."
    },
    "description": "DX Commons Javascript loader",
    "main": "index.js",
    "repository": "git@github.com:Jahia/dx-commons-webpack.git",
    "author": "Jahia Core Team <core@jahia.com>",
    "license": "MIT",
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@jahia/apollo-dx": "^1.0.0-beta.20190222132048",
        "@jahia/ds-mui-theme": "^1.0.0-beta.20190423144545",
        "@jahia/i18next": "^1.0.0-beta.20180628145923",
        "@jahia/icons": "^1.0.0-beta.20190218105956",
        "@jahia/layouts": "^1.0.0-beta.20190401120852",
        "@jahia/react-apollo": "^1.0.0-beta.20190325151341",
        "@jahia/react-material": "^1.0.0-beta.20190409130841",
        "@jahia/registry": "^1.0.0-beta.20190130110253",
        "react": "^16.8.6",
        "react-apollo": "^2.1.3",
        "react-dom": "^16.8.6",
        "redux": "^4.0.0",
        "rxjs": "^6.3.3",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/plugin-external-helpers": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-transform-imports": "^1.5.1",
        "clean-webpack-plugin": "^1.0.0",
