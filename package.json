        "null-loader": "^0.1.1",
        "rimraf": "^2.6.2",
        "style-loader": "^0.23.1",
        "transform-imports": "^2.0.0",
        "webpack": "^4.26.0",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-node-externals": "^1.7.2"
