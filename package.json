{
  "name": "@jahia/dx-commons-webpack",
  "version": "1.2.0",
  "scripts": {
    "build": "yarn test && yarn webpack && yarn webpack:manifest",
    "webpack": "npx --node-arg=--max_old_space_size=2048 webpack",
    "webpack:manifest": "npx --node-arg=--max_old_space_size=2048 webpack --manifest",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn test && yarn webpack -p && yarn webpack:manifest -p",
    "build:production-analyze": "yarn build -p --analyze",
    "watch": "yarn webpack --watch",
    "clean": "rimraf *.log src/main/resources/javascript/apps",
    "clean:all": "yarn clean && rimraf node_modules node",
    "test": "yarn lint",
    "lint": "eslint --ext js,jsx .",
    "lint:fix": "eslint --ext js,jsx --fix ."
  },
  "description": "DX Commons Javascript loader",
  "main": "index.js",
  "repository": "git@github.com:Jahia/dx-commons-webpack.git",
  "author": "Jahia Core Team <core@jahia.com>",
  "license": "MIT",
